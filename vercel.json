{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/ml/(.*)",
            "destination": "/api/ml.py"
        },
        {
            "source": "/((?!api|ml|.*\\.).*)",
            "destination": "/index.html"
        }
    ]
}