{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/ml/(.*)",
            "destination": "/api/ml.py"
        },
        {
            "source": "/((?!api|ml|assets|sw.js|favicon.ico|manifest.json|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}